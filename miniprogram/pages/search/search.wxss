/* miniprogram/pages/search/search.wxss */
.song_box{
  width:100%;
  height:150rpx;
  border:1px solid red;
  display:flex;
  justify-content: space-between;
  flex-flow: row nowrap;
  align-items: center;
  padding:0 40rpx;
}
.song_name{
  width:400rpx;
  height:80rpx;
  display: flex;
  justify-content: space-around;
  flex-flow: column nowrap;
  font-size:28rpx;
}
.song_play{
  width:50rpx;
  height:40rpx;
}
.ydname{
  font-size: 24rpx;
  color:#ccc;
}